FROM jess/chrome

#
# v1 dex-api
#

LABEL \
  org.dockerland.dex.api="v1"
  org.dockerland.dex.docker_flags="\$DEX_X11_FLAGS --cpuset-cpus=0 --network=host"
  org.dockerland.dex.docker_devices="/dev/snd"
  org.dockerland.dex.docker_volumes="/dev/shm /etc/localtime:/etc/localtime:ro"

#
# chrome image
#

CMD [""]
